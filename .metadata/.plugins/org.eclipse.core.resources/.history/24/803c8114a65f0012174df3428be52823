protected function downHandler(event:MouseEvent):void
{
  (this.parentDocument as PolicyProperties).changeOrder(false);
}
protected function _upHandler(event:MouseEvent):void
{
  (this.parentDocument as PolicyProperties).changeOrder(true);
}

